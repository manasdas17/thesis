  doi = {10.1029/96RS00088},
  file = {Snapshot:/home/ryan/.mozilla/firefox/ov15x6uz.default/zotero/storage/JRIP5E4E/abstract.html:text/html;Wannberg_Pellinen-Wannberg_Westman_1996 - An ambiguity-function-based method for analysis of Doppler decompressed radar signals applied to EISCAT measurements of oblique UHF-VHF meteor echoes.pdf:/home/ryan/Documents/Papers/Wannberg_Pellinen-Wannberg_Westman_1996 - An ambiguity-function-based method for analysis of Doppler decompressed radar signals applied to EISCAT measurements of oblique UHF-VHF meteor echoes.pdf:application/pdf},
  issn = {1944-799X},
  keywords = {6245 Planetology: Solar System Objects: Meteors, 6952 Radar atmospheric
